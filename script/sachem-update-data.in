#!/bin/bash

CLASSPATH=@CLASSPATH@

if [ "$#" -ne 1 ]; then
    echo usage: sachem-update-data configfile.properties
    exit 1
fi

if [ ! -f "$1" ]; then
    echo "$1" is missing
    exit 1
fi

java -classpath $CLASSPATH/commons-net-3.9.0.jar:$CLASSPATH/postgresql-42.6.0.jar:$CLASSPATH/sachem.jar cz.iocb.sachem.load.CompoundUpdater "$1"
